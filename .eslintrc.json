{
  "extends": ["airbnb", "prettier", "plugins:node/recommended"],
  "plugins": ["prettier"],

  "rules": {
    "prettier-prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "prefer-arrow-callback": "warn",
    "no-process-exit": "off",
    "class-methods-use-this": "off"
  }
}
